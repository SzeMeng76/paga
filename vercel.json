{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "paga/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/paga/(.*)",
      "dest": "/paga/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}
